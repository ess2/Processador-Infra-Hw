module mux
(output logic Out,
 input logic A, B, IorD);
 
 not #2 o4(n_IorD,IorD);
 and #2 o1(Out1,A, n_IorD),
		o2(Out2,B, IorD);
 or #2 o3(Out,Out1,Out2);

endmodule: mux 